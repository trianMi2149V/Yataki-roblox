local ThemeDropdown = Tabs.Settings:AddDropdown("ThemeDropdown", {
    Title = "Select Theme",
    Values = {
        {Text = "Dark", Color = Color3.fromRGB(33, 33, 33)},
        {Text = "Light", Color = Color3.fromRGB(255, 255, 255)},
        {Text = "Amethyst", Color = Color3.fromRGB(153, 102, 255)},
        {Text = "Red", Color = Color3.fromRGB(255, 0, 0)},
        {Text = "Blue", Color = Color3.fromRGB(0, 0, 255)},
        {Text = "Green", Color = Color3.fromRGB(0, 255, 0)},
        {Text = "Orange", Color = Color3.fromRGB(255, 165, 0)},
        {Text = "Purple", Color = Color3.fromRGB(128, 0, 128)},
        {Text = "Pink", Color = Color3.fromRGB(255, 105, 180)},
        {Text = "Yellow", Color = Color3.fromRGB(255, 255, 0)},
        {Text = "Brown", Color = Color3.fromRGB(139, 69, 19)},
        {Text = "Cyan", Color = Color3.fromRGB(0, 255, 255)},
        {Text = "Lime", Color = Color3.fromRGB(0, 255, 0)},
        {Text = "Teal", Color = Color3.fromRGB(0, 128, 128)},
    },
    Default = "Dark",
    Callback = function(Value)
        if Value.Text == "Dark" then
            Fluent:SetTheme("Dark")
        elseif Value.Text == "Light" then
            Fluent:SetTheme("Light")
        elseif Value.Text == "Amethyst" then
            Fluent:SetTheme("Amethyst")
        elseif Value.Text == "Red" then
            Fluent:SetTheme("Red")
        elseif Value.Text == "Blue" then
            Fluent:SetTheme("Blue")
        elseif Value.Text == "Green" then
            Fluent:SetTheme("Green")
        elseif Value.Text == "Orange" then
            Fluent:SetTheme("Orange")
        elseif Value.Text == "Purple" then
            Fluent:SetTheme("Purple")
        elseif Value.Text == "Pink" then
            Fluent:SetTheme("Pink")
        elseif Value.Text == "Yellow" then
            Fluent:SetTheme("Yellow")
        elseif Value.Text == "Brown" then
            Fluent:SetTheme("Brown")
        elseif Value.Text == "Cyan" then
            Fluent:SetTheme("Cyan")
        elseif Value.Text == "Lime" then
            Fluent:SetTheme("Lime")
        elseif Value.Text == "Teal" then
            Fluent:SetTheme("Teal")
        end
        print("N·ªÅn,Ch·ªØ ƒë√£ thay:", Value.Text)
    end
})

